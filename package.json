{
  "name": "utilitarianism.net",
  "version": "1.0.0",
  "description": "Official repository for utilitarianism.net",
  "keywords": [
    "utilitarianism",
    "utilitarian",
    "hugo"
  ],
  "author": "Boris Yakubchik",
  "scripts": {
    "start": "hugo serve",
    "build": "just build",
    "search": "pagefind --source public",
    "pdf": "cd pdf && website2pdf --marginTop 50px --marginBottom 50px --marginLeft 40px --marginRight 40px --display-header-footer --safe-title --output-dir ./temp && npm run move-pdf",
    "move-pdf": "cd pdf && node move-pdf.js",
    "kill-hugo": "kill-port 1313",
    "public": "serve public",
    "zip": "node build.js zip",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "bestzip": "2.2.1",
    "fdir": "6.0.1",
    "kill-port": "2.0.1",
    "pagefind": "0.11.0",
    "prettier": "2.8.4",
    "prettier-plugin-go-template": "0.0.13",
    "serve": "14.2.0",
    "website2pdf": "0.0.11"
  }
}
